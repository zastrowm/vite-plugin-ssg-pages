{
  "name": "vite-plugin-ssg-pages",
  "version": "0.0.1",
  "packageManager": "yarn@3.6.0",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zastrowm/vite-plugin-ssg-pages.git"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "format": "prettier src/",
    "format:fix": "prettier --write src/",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "files": [
    "dist/**/*"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": true
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "18",
    "preact": "^10.19.3",
    "preact-render-to-string": "^6.3.1",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  },
  "peerDependencies": {
    "vite": "*"
  },
  "optionalDependencies": {
    "preact": "*",
    "preact-render-to-string": "*"
  }
}
